# Copy this file to .env and fill in your actual values

# Application Settings
APP_NAME=RADAR Backend
ENVIRONMENT=PRODUCTION
LOG_LEVEL=INFO

# Elasticsearch Configuration
ELASTIC_HOST=https://elasticsearch:9200
